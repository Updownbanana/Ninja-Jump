    pounce = false
    audio_play_sound(snd_Slash,2,false)
    
    //Get score from robot based on its object name, then destroy it
    with (robot) {
        robot_name = object_get_name(object_index);
        switch (robot_name) {
            case "obj_Robot":           score += 10; break;
            case "obj_Robot_exploding": score += 20; break;
        }
        instance_destroy()
    }
    
    //Create robot halves
